			<reaction name="T0_production" reversible="false">
				<listOfReactants>
					<specieReference specie="AA" stoichiometry="1"/>
				</listOfReactants>
				<listOfProducts>
					<specieReference specie="T0" stoichiometry="1"/>
				</listOfProducts>
				<kineticLaw formula="((AA)*(k_22))*(Mt)">
					<listOfParameters>
						<parameter name="k_22" value="0.9"/>
