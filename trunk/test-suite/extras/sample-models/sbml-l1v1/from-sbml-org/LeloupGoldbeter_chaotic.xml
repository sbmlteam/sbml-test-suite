				<kineticLaw formula="V_1*P0/(Km_1+P0)">
					<listOfParameters>
						<parameter name="Km_1" value="2"/>
						<parameter name="V_1" value="8"/>
					</listOfParameters>
				</kineticLaw>
			</reaction>
			<reaction name="T0_to_T1" reversible="false">
