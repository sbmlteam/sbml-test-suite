			<reaction name="T1_to_T0" reversible="false">
				<listOfReactants>
					<specieReference specie="T1" stoichiometry="1"/>
				</listOfReactants>
				<listOfProducts>
					<specieReference specie="T0" stoichiometry="1"/>
				</listOfProducts>
				<kineticLaw formula="V_4*T1/(Km_4+T1)">
					<listOfParameters>
						<parameter name="Km_4" value="2"/>
