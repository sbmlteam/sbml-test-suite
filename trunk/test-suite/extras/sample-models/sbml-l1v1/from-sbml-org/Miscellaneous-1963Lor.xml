	</notes>
	<listOfCompartments>
		<compartment name="atmosphere"/>
	</listOfCompartments>
	<listOfSpecies>
		<specie name="EmptySet" initialAmount="0" boundaryCondition="true" compartment="atmosphere"/>
		<specie name="x" initialAmount="0" compartment="atmosphere" boundaryCondition="false" />
		<specie name="y" initialAmount="1" compartment="atmosphere" boundaryCondition="false" />
		<specie name="z" initialAmount="0" compartment="atmosphere" boundaryCondition="false" />
	</listOfSpecies>
	<listOfReactions>
		<reaction name="Reaction1" reversible="false">
			<listOfReactants>
				<specieReference specie="EmptySet" stoichiometry="1" />
			</listOfReactants>
			<listOfProducts>
				<specieReference specie="x" />
			</listOfProducts>
			<kineticLaw formula="sigma*(-x + y)">
				<listOfParameters>
					<parameter name="sigma" value="3" />
				</listOfParameters>
			</kineticLaw>
		</reaction>
		<reaction name="Reaction2" reversible="false">
			<listOfReactants>
				<specieReference specie="EmptySet" stoichiometry="1" />
			</listOfReactants>
			<listOfProducts>
				<specieReference specie="y" />
			</listOfProducts>
			<kineticLaw formula="r*x - y - x*z">
				<listOfParameters>
					<parameter name="r" value="26.5" />
				</listOfParameters>
			</kineticLaw>
		</reaction>
		<reaction name="Reaction3" reversible="false">
			<listOfReactants>
				<specieReference specie="EmptySet" stoichiometry="1" />
			</listOfReactants>
			<listOfProducts>
				<specieReference specie="z" />
			</listOfProducts>
			<kineticLaw formula="x*y - b*z">
				<listOfParameters>
					<parameter name="b" value="1" />
				</listOfParameters>
			</kineticLaw>
		</reaction>
	</listOfReactions>
</model>
