<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model>
    <listOfFunctionDefinitions>
      <functionDefinition id="add">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <lambda>
            <bvar> <ci> x </ci> </bvar>
            <apply>
              <plus/> 
              <ci> x </ci> 
              <apply>
                <ci> add </ci>
                <ci> x </ci>
              </apply>
            </apply>
          </lambda>
        </math>
      </functionDefinition>
    </listOfFunctionDefinitions>
  </model>
</sbml>
