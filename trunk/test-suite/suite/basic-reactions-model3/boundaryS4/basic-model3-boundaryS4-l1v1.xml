<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
    <model name="basic_model3_boundaryS4">
        <listOfCompartments>
            <compartment name="c" volume="1"/>
        </listOfCompartments>
        <listOfSpecies>
            <specie name="S1" compartment="c" initialAmount="1e-015"/>
            <specie name="S2" compartment="c" initialAmount="2e-015"/>
            <specie name="S3" compartment="c" initialAmount="1e-015"/>
            <specie name="S4" compartment="c" initialAmount="1e-015" boundaryCondition="true"/>
        </listOfSpecies>
        <listOfReactions>
            <reaction name="R1" reversible="false">
                <listOfReactants>
                    <specieReference specie="S1" stoichiometry="1"/>
                    <specieReference specie="S2" stoichiometry="1"/>
                </listOfReactants>
                <listOfProducts>
                    <specieReference specie="S3" stoichiometry="1"/>
                    <specieReference specie="S4" stoichiometry="1"/>
                </listOfProducts>
                <kineticLaw formula="kf*S1*S2">
                    <listOfParameters>
                        <parameter name="kf" value="0.75e15"/>
                    </listOfParameters>
                </kineticLaw>
            </reaction>
            <reaction name="R2" reversible="false">
                <listOfReactants>
                    <specieReference specie="S3" stoichiometry="1"/>
                    <specieReference specie="S4" stoichiometry="1"/>
                </listOfReactants>
                <listOfProducts>
                    <specieReference specie="S1" stoichiometry="1"/>
                    <specieReference specie="S2" stoichiometry="1"/>
                </listOfProducts>
                <kineticLaw formula="kr*S3*S4">
                    <listOfParameters>
                        <parameter name="kr" value="0.25e15"/>
                    </listOfParameters>
                </kineticLaw>
            </reaction>
        </listOfReactions>
    </model>
</sbml>
