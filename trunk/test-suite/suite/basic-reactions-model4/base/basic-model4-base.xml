<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
  <model name="basic_model4_base">
    <listOfCompartments>
      <compartment name="compartment" volume="1"/>
      <compartment name="c" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="S1" compartment="c" initialAmount="1e-015"/>
      <species name="S2" compartment="c" initialAmount="2e-15"/>
      <species name="S3" compartment="c" initialAmount="0"/>
      <species name="S4" compartment="c" initialAmount="0"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="R1" reversible="false">
        <listOfReactants>
          <speciesReference species="S1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw formula="k1f * S1">
          <listOfParameters>
            <parameter name="k1f" value="0.75"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="R2" reversible="false">
        <listOfReactants>
          <speciesReference species="S2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S1"/>
        </listOfProducts>
        <kineticLaw formula="k1r * S2">
          <listOfParameters>
            <parameter name="k1r" value="0.25"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="R3" reversible="false">
        <listOfReactants>
          <speciesReference species="S2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S3"/>
          <speciesReference species="S4"/>
        </listOfProducts>
        <kineticLaw formula="k2f * S2">
          <listOfParameters>
            <parameter name="k2f" value="0.15"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="R4" reversible="false">
        <listOfReactants>
          <speciesReference species="S3"/>
          <speciesReference species="S4"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw formula="k2r * S3 * S4">
          <listOfParameters>
            <parameter name="k2r" value="0.1"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
