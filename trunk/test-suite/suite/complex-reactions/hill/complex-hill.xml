<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
   <model name = "complexReactions_hill">
    <listOfCompartments>
      <compartment name="compartment" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="S1" compartment="compartment" initialAmount="0"/>
      <species name="S2" compartment="compartment" initialAmount="0"/>
      <species name="S3" compartment="compartment" initialAmount="0"/>
      <species name="S4" compartment="compartment" initialAmount="0" boundaryCondition="true"/>
      <species name="S0" compartment="compartment" initialAmount="5" boundaryCondition="true"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="J1" reversible="false">
        <listOfReactants>
          <speciesReference species="S1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw formula="Vmax * pow(S1, n) / (pow(K, n) + pow(S1, n))">
          <listOfParameters>
            <parameter name="Vmax" value="5.5"/>
            <parameter name="n" value="4"/>
            <parameter name="K" value="0.5"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="J2" reversible="false">
        <listOfReactants>
          <speciesReference species="S2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S3"/>
        </listOfProducts>
        <kineticLaw formula="J2_k * S2">
          <listOfParameters>
            <parameter name="J2_k" value="0.1"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="J3" reversible="false">
        <listOfReactants>
          <speciesReference species="S3"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S4"/>
        </listOfProducts>
        <kineticLaw formula="J3_k * S3">
          <listOfParameters>
            <parameter name="J3_k" value="0.1"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="J0" reversible="false">
        <listOfReactants>
          <speciesReference species="S0"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S1"/>
        </listOfProducts>
        <kineticLaw formula="J0_k * S0">
          <listOfParameters>
            <parameter name="J0_k" value="0.01"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
