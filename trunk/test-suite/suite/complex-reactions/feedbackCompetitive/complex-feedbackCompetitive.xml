<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
   <model name = "complexReactions_feedbackCompetitive">
    <listOfCompartments>
      <compartment name="compartment" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="S1" compartment="compartment" initialAmount="0"/>
      <species name="S2" compartment="compartment" initialAmount="0"/>
      <species name="S3" compartment="compartment" initialAmount="0"/>
      <species name="S4" compartment="compartment" initialAmount="0"/>
      <species name="X0" compartment="compartment" initialAmount="10" boundaryCondition="true"/>
      <species name="X1" compartment="compartment" initialAmount="0" boundaryCondition="true"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="J0" reversible="false">
        <listOfReactants>
          <speciesReference species="X0"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S1"/>
        </listOfProducts>
        <kineticLaw formula="VM1 * (X0 - S1 / Keq1) / (1 + X0 + S1 + pow(S4, h))">
          <listOfParameters>
            <parameter name="VM1" value="10"/>
            <parameter name="Keq1" value="10"/>
            <parameter name="h" value="10"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="J1" reversible="false">
        <listOfReactants>
          <speciesReference species="S1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw formula="(10 * S1 - 2 * S2) / (1 + S1 + S2)"/>
      </reaction>
      <reaction name="J2" reversible="false">
        <listOfReactants>
          <speciesReference species="S2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S3"/>
        </listOfProducts>
        <kineticLaw formula="(10 * S2 - 2 * S3) / (1 + S2 + S3)"/>
      </reaction>
      <reaction name="J3" reversible="false">
        <listOfReactants>
          <speciesReference species="S3"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S4"/>
        </listOfProducts>
        <kineticLaw formula="(10 * S3 - 2 * S4) / (1 + S3 + S4)"/>
      </reaction>
      <reaction name="J4" reversible="false">
        <listOfReactants>
          <speciesReference species="S4"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="X1"/>
        </listOfProducts>
        <kineticLaw formula="V4 * S4 / (KS4 + S4)">
          <listOfParameters>
            <parameter name="V4" value="2.5"/>
            <parameter name="KS4" value="0.5"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
