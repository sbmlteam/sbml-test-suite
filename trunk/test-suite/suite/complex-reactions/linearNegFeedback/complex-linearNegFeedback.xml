<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
   <model name = "complexReactions_linearNegFeedback">
    <listOfCompartments>
      <compartment name="compartment" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="Substrate" compartment="compartment" initialAmount="100" boundaryCondition="true"/>
      <species name="Catalyst" compartment="compartment" initialAmount="100"/>
      <species name="Complex" compartment="compartment" initialAmount="1"/>
      <species name="Product" compartment="compartment" initialAmount="1"/>
      <species name="IntermediateProduct1" compartment="compartment" initialAmount="0"/>
      <species name="IntermediateProduct2" compartment="compartment" initialAmount="0"/>
      <species name="IntermediateProduct3" compartment="compartment" initialAmount="0"/>
      <species name="IntermediateProduct4" compartment="compartment" initialAmount="0"/>
      <species name="ModifiedProduct" compartment="compartment" initialAmount="0"/>
      <species name="Junk" compartment="compartment" initialAmount="100" boundaryCondition="true"/>
      <species name="SequestrationComplex" compartment="compartment" initialAmount="0"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="complexFormation" reversible="false">
        <listOfReactants>
          <speciesReference species="Catalyst"/>
          <speciesReference species="Substrate"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Complex"/>
        </listOfProducts>
        <kineticLaw formula="kf * Catalyst * Substrate">
          <listOfParameters>
            <parameter name="kf" value="0.005"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="Catalysis" reversible="false">
        <listOfReactants>
          <speciesReference species="Complex"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Product"/>
          <speciesReference species="Catalyst"/>
        </listOfProducts>
        <kineticLaw formula="kcat * Complex">
          <listOfParameters>
            <parameter name="kcat" value="0.06"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="productPathway1" reversible="false">
        <listOfReactants>
          <speciesReference species="Product"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="IntermediateProduct1"/>
        </listOfProducts>
        <kineticLaw formula="kpath1 * Product">
          <listOfParameters>
            <parameter name="kpath1" value="0.003"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="productPathway2" reversible="false">
        <listOfReactants>
          <speciesReference species="IntermediateProduct1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="IntermediateProduct2"/>
        </listOfProducts>
        <kineticLaw formula="kpath2 * IntermediateProduct1">
          <listOfParameters>
            <parameter name="kpath2" value="0.003"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="productPathway3" reversible="false">
        <listOfReactants>
          <speciesReference species="IntermediateProduct2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="IntermediateProduct3"/>
        </listOfProducts>
        <kineticLaw formula="kpath3 * IntermediateProduct2">
          <listOfParameters>
            <parameter name="kpath3" value="0.003"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="productPathway4" reversible="false">
        <listOfReactants>
          <speciesReference species="IntermediateProduct3"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="IntermediateProduct4"/>
        </listOfProducts>
        <kineticLaw formula="kpath4 * IntermediateProduct3">
          <listOfParameters>
            <parameter name="kpath4" value="0.003"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="productModification" reversible="false">
        <listOfReactants>
          <speciesReference species="IntermediateProduct4"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="ModifiedProduct"/>
        </listOfProducts>
        <kineticLaw formula="kmod * IntermediateProduct4">
          <listOfParameters>
            <parameter name="kmod" value="0.003"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="sequestration" reversible="false">
        <listOfReactants>
          <speciesReference species="Catalyst"/>
          <speciesReference species="ModifiedProduct"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="SequestrationComplex"/>
        </listOfProducts>
        <kineticLaw formula="kseq * Catalyst * ModifiedProduct">
          <listOfParameters>
            <parameter name="kseq" value="0.5"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="Unsequestration" reversible="false">
        <listOfReactants>
          <speciesReference species="SequestrationComplex"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="ModifiedProduct"/>
          <speciesReference species="Catalyst"/>
        </listOfProducts>
        <kineticLaw formula="kunseq * SequestrationComplex">
          <listOfParameters>
            <parameter name="kunseq" value="0.01"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="complexSeperation" reversible="false">
        <listOfReactants>
          <speciesReference species="Complex"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Substrate"/>
          <speciesReference species="Catalyst"/>
        </listOfProducts>
        <kineticLaw formula="kr * Complex">
          <listOfParameters>
            <parameter name="kr" value="0.01"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="degradation" reversible="false">
        <listOfReactants>
          <speciesReference species="ModifiedProduct"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Junk"/>
        </listOfProducts>
        <kineticLaw formula="kdeg * ModifiedProduct">
          <listOfParameters>
            <parameter name="kdeg" value="0.005"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
