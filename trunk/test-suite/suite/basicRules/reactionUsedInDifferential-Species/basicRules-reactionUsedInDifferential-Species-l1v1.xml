<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
    <model name="basicRules_reactionUsedInDifferential_Species">
        <listOfCompartments>
            <compartment name="c" volume="1"/>
        </listOfCompartments>
        <listOfSpecies>
            <specie name="s1" compartment="c" initialAmount="10" boundaryCondition="false"/>
            <specie name="s2" compartment="c" initialAmount="0" boundaryCondition="true"/>
        </listOfSpecies>
        <listOfRules>
            <specieConcentrationRule specie="s2" type="rate" formula="10*s1"/>
        </listOfRules>
        <listOfReactions>
            <reaction name="r">
                <listOfReactants>
                    <specieReference specie="s1"/>
                </listOfReactants>
                <listOfProducts>
                    <specieReference specie="s2"/>
                </listOfProducts>
                <kineticLaw formula="5*s1"/>
            </reaction>
        </listOfReactions>
    </model>
</sbml>
