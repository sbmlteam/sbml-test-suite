<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
  <model name="basicRules_dynamicAssignmentUsedInDifferential_Species">
    <listOfCompartments>
      <compartment name="c" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="s1" compartment="c" initialAmount="0"/>
      <species name="s2" compartment="c" initialAmount="0"/>
    </listOfSpecies>
    <listOfRules>
      <speciesConcentrationRule formula="0.003 * s2" species="s1"/>
      <speciesConcentrationRule formula="0.005 * s1 + 0.01" type="rate" species="s2"/>
    </listOfRules>
  </model>
</sbml>
