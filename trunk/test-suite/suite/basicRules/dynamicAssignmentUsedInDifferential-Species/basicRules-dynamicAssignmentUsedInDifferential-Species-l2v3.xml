<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version3" level="2" version="3">
  <model id="basicRules_dynamicAssignmentUsedInDifferential_Species">
    <listOfCompartments>
      <compartment id="c" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s1" compartment="c" initialAmount="0"/>
      <species id="s2" compartment="c" initialAmount="0"/>
    </listOfSpecies>
    <listOfRules>
      <assignmentRule variable="s1">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn> 0.003 </cn>
            <ci> s2 </ci>
          </apply>
        </math>
      </assignmentRule>
      <rateRule variable="s2">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <apply>
              <times/>
              <cn> 0.005 </cn>
              <ci> s1 </ci>
            </apply>
            <cn> 0.01 </cn>
          </apply>
        </math>
      </rateRule>
    </listOfRules>
  </model>
</sbml>
