<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
    <model name="basicRules_dynamicAssignmentUsedInReaction_Species">
        <listOfCompartments>
            <compartment name="c" volume="1"/>
        </listOfCompartments>
        <listOfSpecies>
            <specie name="s1" compartment="c" initialAmount="0" boundaryCondition="true"/>
            <specie name="s2" compartment="c" initialAmount="0" boundaryCondition="false"/>
        </listOfSpecies>
        <listOfRules>
            <specieConcentrationRule specie="s1" type="scalar" formula="0.003 * s2"/>
        </listOfRules>
        <listOfReactions>
            <reaction name="r">
                <listOfReactants>
                    <specieReference specie="s1"/>
                </listOfReactants>
                <listOfProducts>
                    <specieReference specie="s2"/>
                </listOfProducts>
                <kineticLaw formula="0.005*s1 + 0.01"/>
            </reaction>
        </listOfReactions>
    </model>
</sbml>
