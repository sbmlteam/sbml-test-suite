<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model id="basicRules_dynamicAssignmentUsedInReaction_Species">
    <listOfCompartments>
      <compartment id="c" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s1" compartment="c" initialAmount="0" boundaryCondition="true"/>
      <species id="s2" compartment="c" initialAmount="0"/>
    </listOfSpecies>
    <listOfRules>
      <assignmentRule variable="s1">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn> 0.003 </cn>
            <ci> s2 </ci>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
    <listOfReactions>
      <reaction id="r">
        <listOfReactants>
          <speciesReference species="s1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="s2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <plus/>
              <apply>
                <times/>
                <cn> 0.005 </cn>
                <ci> s1 </ci>
              </apply>
              <cn> 0.01 </cn>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
