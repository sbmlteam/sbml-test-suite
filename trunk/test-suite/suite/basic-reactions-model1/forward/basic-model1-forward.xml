<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
	<model name="basic_model1_forward">
    <listOfCompartments>
		<compartment name="compartment" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
		<species name="S1" compartment="compartment" initialAmount="1.5e-015" boundaryCondition="false"/>
		<species name="S2" compartment="compartment" initialAmount="1.5e-015" boundaryCondition="false"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="R1" reversible="false">
        <listOfReactants>
          <speciesReference species="S1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw formula="k_1 * S1">
          <listOfParameters>
            <parameter name="k_1" value="1"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="R2" reversible="false">
        <listOfReactants>
          <speciesReference species="S2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S1"/>
        </listOfProducts>
        <kineticLaw formula="k_2 * S2">
          <listOfParameters>
            <parameter name="k_2" value="0"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
