<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="1">
    <model name="basic_model1_stoichiometryIs2">
        <listOfCompartments>
            <compartment name="c" volume="1"/>
        </listOfCompartments>
        <listOfSpecies>
            <specie name="S1" compartment="c" initialAmount="1.5e-015"/>
            <specie name="S2" compartment="c" initialAmount="1.5e-015"/>
        </listOfSpecies>
        <listOfReactions>
            <reaction name="R1" reversible="false">
                <listOfReactants>
                    <specieReference specie="S1" stoichiometry="2"/>
                </listOfReactants>
                <listOfProducts>
                    <specieReference specie="S2" />
                </listOfProducts>
                <kineticLaw formula="k_1*S1">
                    <listOfParameters>
                        <parameter name="k_1" value="0.5"/>
                    </listOfParameters>
                </kineticLaw>
            </reaction>
            <reaction name="R2" reversible="false">
                <listOfReactants>
                    <specieReference specie="S2"/>
                </listOfReactants>
                <listOfProducts>
                    <specieReference specie="S1" stoichiometry="2"/>
                </listOfProducts>
                <kineticLaw formula="k_2*S2">
                    <listOfParameters>
                        <parameter name="k_2" value="0.25"/>
                    </listOfParameters>
                </kineticLaw>
            </reaction>
        </listOfReactions>
    </model>
</sbml>
