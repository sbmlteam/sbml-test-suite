<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
<model id="algebraicRules_basic">
  <listOfCompartments>
    <compartment id="cell" size="1"/>
  </listOfCompartments>
  <listOfSpecies>
    <species id="x" compartment="cell" initialConcentration="0.5"/>
    <species id="y" compartment="cell" initialConcentration="0.5"/>
    <species id="z" compartment="cell" initialConcentration="1" constant="true"/>
  </listOfSpecies>
  <listOfRules>
    <rateRule variable="x">
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <times/>
          <ci> y </ci>
          <ci> z </ci>
        </apply>
      </math>
    </rateRule>
    <algebraicRule>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <minus/>
          <apply>
            <plus/>
            <ci> x </ci>
            <ci> y </ci>
          </apply>
          <ci> z </ci>
        </apply>
      </math>
    </algebraicRule>
  </listOfRules>
</model>
</sbml>
