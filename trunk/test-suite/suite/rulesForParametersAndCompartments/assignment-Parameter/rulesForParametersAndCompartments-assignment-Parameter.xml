    <listOfCompartments>
      <compartment name="c" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="sout" compartment="c" initialAmount="0"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter name="p" value="7"/>
