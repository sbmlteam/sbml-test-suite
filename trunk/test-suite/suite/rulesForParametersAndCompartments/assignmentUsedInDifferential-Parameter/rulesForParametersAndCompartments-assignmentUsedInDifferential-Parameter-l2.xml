<sbml xmlns="http://www.sbml.org/sbml/level2" level="2" version="1">
  <model id="rulesForParametersAndCompartments_assignmentUsedInDifferential_Parameter">
    <listOfCompartments>
      <compartment id="c" size="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species id="s" compartment="c" initialAmount="0"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="p" constant="false"/>
    </listOfParameters>
    <listOfRules>
      <assignmentRule variable="p">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn type="integer"> 3 </cn>
            <cn type="integer"> s </cn>
          </apply>
        </math>
      </assignmentRule>
      <rateRule variable="s">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <cn type="integer"> 5 </cn>
            <ci> p </ci>
          </apply>
        </math>
      </rateRule>
    </listOfRules>
  </model>
</sbml>
