<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
  <model name="parameterNamespace_global">
    <listOfCompartments>
      <compartment name="compartment" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="S1" compartment="compartment" initialAmount="0"/>
      <species name="S2" compartment="compartment" initialAmount="0"/>
      <species name="S3" compartment="compartment" initialAmount="0"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter name="k_1" value="1"/>
      <parameter name="k_2" value="0.1"/>
    </listOfParameters>
    <listOfReactions>
      <reaction name="R1" reversible="false">
        <listOfReactants>
          <speciesReference species="S1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw formula="k_1 * S1"/>
      </reaction>
      <reaction name="R2" reversible="false">
        <listOfReactants>
          <speciesReference species="S2"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S3"/>
        </listOfProducts>
        <kineticLaw formula="k_2 * S2"/>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
