<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
 <model name = "basic_model6_base">
    <listOfCompartments>
      <compartment name="compartment" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="A" compartment="compartment" initialAmount="2e-15"/>
      <species name="B" compartment="compartment" initialAmount="1e-15"/>
      <species name="C" compartment="compartment" initialAmount="5e-16"/>
      <species name="D" compartment="compartment" initialAmount="0"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="J0" reversible="false">
        <listOfReactants>
          <speciesReference species="A"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="B"/>
        </listOfProducts>
        <kineticLaw formula="k1 * A">
          <listOfParameters>
            <parameter name="k1" value="0.7"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="J1" reversible="false">
        <listOfReactants>
          <speciesReference species="B"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="C"/>
        </listOfProducts>
        <kineticLaw formula="k2 * B">
          <listOfParameters>
            <parameter name="k2" value="0.5"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="J2" reversible="false">
        <listOfReactants>
          <speciesReference species="C"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="D"/>
        </listOfProducts>
        <kineticLaw formula="k3 * C">
          <listOfParameters>
            <parameter name="k3" value="1"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
