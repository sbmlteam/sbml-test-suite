<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
  <model name="discontinuityTimeAndDelay_ceiling">
    <listOfCompartments>
      <compartment name="compartment" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="S1" compartment="compartment" initialAmount="1"/>
      <species name="S2" compartment="compartment" initialAmount="0"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="R" reversible="false">
        <listOfReactants>
          <speciesReference species="S1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2"/>
        </listOfProducts>
        <kineticLaw formula="ceil(S1 * 5) / 5"/>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
