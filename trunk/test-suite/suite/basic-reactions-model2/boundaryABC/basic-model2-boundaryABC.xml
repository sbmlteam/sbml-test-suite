<sbml xmlns="http://www.sbml.org/sbml/level1" level="1" version="2">
  <model name="basic_model2_boundaryABC">
    <listOfCompartments>
      <compartment name="c" volume="1"/>
    </listOfCompartments>
    <listOfSpecies>
      <species name="A" compartment="c" initialAmount="1e-015" boundaryCondition="true"/>
      <species name="B" compartment="c" initialAmount="2e-015" boundaryCondition="true"/>
      <species name="C" compartment="c" initialAmount="1e-015" boundaryCondition="true"/>
    </listOfSpecies>
    <listOfReactions>
      <reaction name="R1" reversible="false">
        <listOfReactants>
          <speciesReference species="A"/>
          <speciesReference species="B"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="C"/>
        </listOfProducts>
        <kineticLaw formula="kf * A * B">
          <listOfParameters>
            <parameter name="kf" value="0.75"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction name="R2" reversible="false">
        <listOfReactants>
          <speciesReference species="C"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="A"/>
          <speciesReference species="B"/>
        </listOfProducts>
        <kineticLaw formula="kr * C">
          <listOfParameters>
            <parameter name="kr" value="0.25"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
